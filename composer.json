{
    "name": "nntoan/valet-pro-max",
    "description": "Blazing fast macOS PHP development environment",
    "license": "MIT",
    "authors": [
        {
            "name": "Mischa Braam",
            "email": "mischa.braam@iodigital.com"
        },
        {
            "name": "Toan Nguyen",
            "email": "github@nntoan.com"
        }
    ],
    "autoload": {
        "files": [
            "cli/includes/facades.php"
        ],
        "psr-4": {
            "Lotus\\ValetProMax\\": "cli/ValetProMax/"
        }
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "phpro/grumphp-shim": true
        }
    },
    "bin": [
        "valet-pro"
    ],
    "require": {
        "php": ">=7.1 <8.3",
        "laravel/valet": "^4.5"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.8",
        "phpro/grumphp-shim": "^2.4"
    }
}
